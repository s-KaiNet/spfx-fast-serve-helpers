{
  "name": "spfx-fast-serve-helpers",
  "version": "1.18.5",
  "description": "Contains helper classes and functions for spfx-fast-serve tool",
  "main": "lib/index.js",
  "bin": {
    "fast-serve": "lib/serve.js"
  },
  "scripts": {
    "build": "npm run lint && tsc -p . && npm run copy",
    "lint": "eslint -c .eslintrc.json --ext .ts src",
    "copy": "cpy **/*.* ../../lib/templates --parents --cwd=src/templates",
    "prepublishOnly": "rimraf -- lib && npm run build",
    "dev": "npm run copy && tsc -p . --watch",
    "pub:beta": "pnpm publish --tag beta",
    "pub:prev": "pnpm publish --tag previous",
    "pub:next": "pnpm publish --tag next"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s-KaiNet/spfx-fast-serve-helpers.git"
  },
  "author": "Sergei Sergeev (https://github.com/s-KaiNet)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/s-KaiNet/spfx-fast-serve-helpers/issues"
  },
  "homepage": "https://github.com/s-KaiNet/spfx-fast-serve-helpers#readme",
  "devDependencies": {
    "@types/node": "15.0.2",
    "@types/node-fetch": "2.6.4",
    "@types/webpack": "4.41.30",
    "@typescript-eslint/eslint-plugin": "5.30.0",
    "@typescript-eslint/parser": "5.30.0",
    "concurrently": "6.2.0",
    "cpy-cli": "3.1.1",
    "eslint": "7.31.0",
    "onchange": "7.1.0",
    "rimraf": "3.0.2",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@microsoft/loader-load-themed-styles": "2.1.7",
    "@microsoft/spfx-heft-plugins": "1.18.2",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.7",
    "@types/copy-webpack-plugin": "6.4.3",
    "@types/cross-spawn": "6.0.3",
    "@types/loader-utils": "2.0.2",
    "@types/webpack-dev-server": "3.11.4",
    "@types/yargs": "6.6.0",
    "autoprefixer": "9.8.8",
    "clean-css-loader": "3.0.0",
    "colors": "1.4.0",
    "commander": "11.1.0",
    "copy-webpack-plugin": "6.4.0",
